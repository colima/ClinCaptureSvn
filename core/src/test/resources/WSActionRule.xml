<RuleImport>
	<RuleAssignment>
		<Target Context="OC_RULES_V1">SED_OID.CRF_OID.ITEM_GROUP_OID.ITEM_OID</Target>
		<RuleRef OID="WS001_RULE">
			<WebServiceAction IfExpressionEvaluates="true">
				<Run AdministrativeDataEntry="true" InitialDataEntry="true"
					DoubleDataEntry="true" ImportDataEntry="false" Batch="false" />

				<username>demosite1</username>
				<password>demo1</password>
				<rolename>admin</rolename>
				<studyoid>SE_TEST</studyoid>
				<studysubjectoid>SE_TEST_SS_NUM</studysubjectoid>
				<authenticationurl>https://www.randomize.net/api/RandomizeAPIService/Authenticate</authenticationurl>
				<randomizationurl>https://www.randomize.net/api//RandomizeAPIService/RandomizePatientDelegated</randomizationurl>
			</WebServiceAction>
		</RuleRef>
	</RuleAssignment>

	<RuleDef OID="WS001_RULE" Name="Web service rule action">
		<Description>Fires the web service call for randomizing a Subject</Description>
		<Expression>ITEM_OID eq "Yes"</Expression>
	</RuleDef>
</RuleImport>