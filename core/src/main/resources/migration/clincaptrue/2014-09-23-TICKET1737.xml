<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="skirpichenok" id="2014-09-23-TICKET1737-01">
        <comment>Add support of new theme on the system settings page</comment>
        <update tableName="system">
            <column name="type_values" value="blue,darkBlue,green,violet"/>
            <where>name = 'themeColor'</where>
        </update>
    </changeSet>

</databaseChangeLog>