                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="vitaly" id="2013-01-15-TICKET875">
        <comment>Insert the study parameter for bioontology verification</comment>
        <insert tableName="study_parameter">
            <column name="study_parameter_id" valueNumeric="39" />
            <column name="handle" value="medicalCodingApiKey" />
            <column name="name" value="medicalCodingApiKey" />
