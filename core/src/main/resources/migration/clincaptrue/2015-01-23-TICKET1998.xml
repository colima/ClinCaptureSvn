<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
				   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

	<changeSet author="Frank" id="2015-01-23-TICKET1998">
		<comment>Adds study parameter to allow/deny Discrepancy Correction Forms</comment>
		<insert tableName="study_parameter">
			<column name="study_parameter_id" value="50" />
			<column name="handle" value="allowDiscrepancyCorrectionForms" />
			<column name="name" value="allowDiscrepancyCorrectionForms" />
			<column name="description" value="Allows/denies Discrepancy Correction Forms" />
			<column name="default_value" value="yes" />
			<column name="inheritable" value="TRUE" />
			<column name="overridable" value="FALSE" />
		</insert>
	</changeSet>
</databaseChangeLog>
